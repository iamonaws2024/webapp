pipeline {
    agent any
    tools {
        jdk '11.0.22'
        jdk '21.0.2'
    }
    stages {
        stage('java 11') {
            steps {
                sh 'java -version'
                sh 'javac -version'
            }
        }
        stage('java 21') {
            steps {
                sh 'java -version'
                sh 'javac -version'
            }
        }
    }
}

